{
  "name": "@primate/live",
  "version": "0.1.0",
  "description": "Primate live server values module",
  "homepage": "https://primatejs.com/modules/live",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/live"
  },
  "dependencies": {},
  "peerDependencies": {
    "primate": "0.32"
  },
  "type": "module",
  "imports": {
    "#*": {
      "@primate/lt": "./src/private/*.js",
      "default": "./src/private/*.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./src/public/runtime.js",
      "default": "./src/public/default.js"
    },
    "./*": {
      "runtime": "./src/public/*.js",
      "default": "./src/public/*.js"
    }
  }
}
