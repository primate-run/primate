{
  "name": "@primate/angular",
  "version": "0.6.0-pre",
  "description": "Angular for Primate",
  "homepage": "https://primate.run/docs/frontend/angular",
  "bugs": "https://github.com/primate-run/primate/issues",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/angular"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@angular/common": "^21.1.4",
    "@angular/compiler": "^21.1.4",
    "@angular/core": "^21.1.4",
    "@angular/platform-browser": "^21.1.4",
    "@angular/platform-server": "^21.1.4",
    "@angular/ssr": "^21.1.4",
    "@primate/core": "workspace:^",
    "typescript": "^5.9.3"
  },
  "devDependencies": {
    "@rcompat/type": "^0.9.0"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "browser": "./lib/public/browser.js",
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
