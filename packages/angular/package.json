{
  "name": "@primate/angular",
  "version": "0.2.0",
  "description": "Primate Angular frontend",
  "homepage": "https://primate.run/docs/frontend/angular",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/angular"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@angular/compiler": "^20.1.6",
    "@angular/platform-browser": "^20.1.6",
    "@angular/platform-server": "^20.1.6",
    "@angular/ssr": "^20.1.5",
    "@primate/core": "workspace:^",
    "@rcompat/assert": "^0.2.0",
    "@rcompat/build": "^0.13.0",
    "@rcompat/crypto": "^0.10.0",
    "@rcompat/fs": "^0.17.0",
    "@rcompat/record": "^0.9.0",
    "zone.js": "^0.15.1"
  },
  "peerDependencies": {
    "@angular/core": "20",
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "browser": "./lib/public/browser.js",
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
