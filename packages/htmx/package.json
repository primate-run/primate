{
  "name": "@primate/htmx",
  "version": "0.19.5",
  "description": "Primate HTMX frontend",
  "homepage": "https://primate.run/docs/frontend/htmx",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/htmx"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@primate/html": "workspace:^",
    "@rcompat/assert": "^0.1.2",
    "@rcompat/fs": "^0.13.2",
    "@rcompat/html": "^0.5.0",
    "@rcompat/record": "^0.8.1",
    "htmx-esm": "^0.2.1910"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
