{
  "name": "@primate/surrealdb",
  "version": "0.7.0",
  "description": "SurrealDB databases for Primate",
  "homepage": "https://primate.run/docs/database/surrealdb",
  "bugs": "https://github.com/primate-run/primate/issues",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/surrealdb"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "npm run build && npx proby"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/assert": "^0.5.0",
    "@surrealdb/node": "1.0.0-beta.3",
    "pema": "workspace:^",
    "surrealdb": "^1.3.2"
  },
  "devDependencies": {
    "@rcompat/type": "^0.8.0"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "apekit": "./src/public/index.ts",
      "default": "./lib/public/index.js"
    }
  }
}
