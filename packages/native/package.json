{
  "name": "@primate/native",
  "version": "0.5.0",
  "description": "Primate native applications",
  "homepage": "https://primate.run/docs/native",
  "bugs": "https://github.com/primate-run/primate/issues",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/native"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "npm run build && npx proby"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/cli": "^0.14.0",
    "@rcompat/io": "^0.3.0",
    "@rcompat/runtime": "^0.9.0",
    "@rcompat/webview": "^0.18.0",
    "pema": "workspace:^"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "apekit": "./src/public/index.ts",
      "default": "./lib/public/index.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
