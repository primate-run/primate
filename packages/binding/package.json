{
  "name": "@primate/binding",
  "version": "0.7.1",
  "description": "Primate binding module",
  "homepage": "https://primatejs.com/modules/binding",
  "bugs": "https://github.com/primatejs/primate/issues",
  "license": "MIT",
  "files": [
    "src/**/*.js",
    "src/**/*.json",
    "src/**/*.go",
    "src/**/*.py",
    "src/**/*.rb",
    "src/**/*.sum",
    "src/**/*.mod",
    "!src/**/*.spec.js"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primatejs/primate",
    "directory": "packages/binding"
  },
  "dependencies": {
    "rcompat": "^0.17.0"
  },
  "peerDependencies": {
    "@ruby/head-wasm-wasi": "2",
    "@ruby/wasm-wasi": "2",
    "primate": "workspace:^",
    "pyodide": "0.25"
  },
  "peerDependenciesMeta": {
    "@ruby/head-wasm-wasi": {
      "optional": true
    },
    "@ruby/wasm-wasi": {
      "optional": true
    },
    "pyodide": {
      "optional": true
    }
  },
  "type": "module",
  "exports": {
    ".": {
      "default": "./src/exports.js"
    },
    "./go": "./src/bindings/go/exports.js",
    "./python": "./src/bindings/python/exports.js",
    "./ruby": "./src/bindings/ruby/exports.js"
  }
}
