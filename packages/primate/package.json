{
  "name": "primate",
  "version": "0.36.0-pre",
  "description": "The universal web framework",
  "homepage": "https://primate.run",
  "bugs": "https://github.com/primate-run/primate/issues",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/primate"
  },
  "files": [
    "/lib/app.tsconfig.json",
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "bin": "lib/bin.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "npm run clean && tsc && cp src/{app.tsconfig.json,index.d.ts} lib",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/args": "^0.11.0",
    "@rcompat/cli": "^0.14.0",
    "@rcompat/dict": "^0.3.1",
    "@rcompat/fs": "^0.25.2",
    "@rcompat/http": "~0.21.0",
    "@rcompat/test": "^0.8.3"
  },
  "devDependencies": {
    "@rcompat/type": "^0.9.0"
  },
  "engines": {
    "node": ">=20"
  },
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "types": "./lib/index.d.ts"
    },
    "./tsconfig": "./lib/app.tsconfig.json",
    "./runtime/*": {
      "apekit": "./src/runtime/*.ts",
      "default": "./lib/runtime/*.js"
    },
    "./http/*": {
      "apekit": "./src/public/http/*.ts",
      "default": "./lib/public/http/*.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
