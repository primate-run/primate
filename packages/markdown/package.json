{
  "name": "@primate/markdown",
  "version": "0.7.0",
  "description": "Markdown for Primate",
  "homepage": "https://primate.run/docs/frontend/markdown",
  "bugs": "https://github.com/primate-run/primate/issues",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/markdown"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/assert": "^0.6.0",
    "@rcompat/string": "^0.13.0",
    "marked": "^17.0.1",
    "pema": "workspace:^"
  },
  "devDependencies": {
    "@rcompat/type": "^0.9.0"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
