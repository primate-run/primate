{
  "name": "@primate/react",
  "version": "0.13.0",
  "description": "Primate React frontend",
  "homepage": "https://primate.run/docs/frontend/react",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/react"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "devDependencies": {
    "@types/react": "^19.1.15",
    "@types/react-dom": "^19.1.9"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/build": "^0.14.0",
    "@rcompat/fs": "^0.21.1",
    "@rcompat/runtime": "^0.6.0"
  },
  "peerDependencies": {
    "primate": "workspace:^",
    "react": "19",
    "react-dom": "19",
    "@types/react": "19",
    "@types/react-dom": "19"

  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "browser": "./lib/public/browser.js",
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": "./lib/public/*.js",
    "./Head": {
      "browser": "./lib/public/Head/browser.js",
      "default": "./lib/public/Head/default.js"
    },
    "./i18n/Bridge": {
      "browser": "./lib/public/i18n/Bridge/browser.js",
      "default": "./lib/public/i18n/Bridge/default.js"
    }
  }
}
