{
  "name": "@primate/poly",
  "version": "0.1.1",
  "description": "Primate Poly frontend",
  "homepage": "https://primate.run/docs/frontend/poly",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/poly"
  },
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/fs": "^0.13.2",
    "pema": "workspace:^"
  },
  "peerDependencies": {
    "@primate/i18n": "workspace:^",
    "poly": "0.7",
    "primate": "workspace:^"
  },
  "peerDependenciesMeta": {
    "@primate/i18n": {
      "optional": true
    }
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    ".": {
      "browser": "./lib/public/browser.js",
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
