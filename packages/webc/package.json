{
  "name": "@primate/webc",
  "version": "0.5.0",
  "description": "Web Components for Primate",
  "homepage": "https://primate.run/docs/frontend/web-components",
  "bugs": "https://github.com/primate-run/primate/issues",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/webc"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "!/**/*.spec.*"
  ],
  "scripts": {
    "build": "npm run clean && tsc",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@primate/core": "workspace:^",
    "@rcompat/fs": "^0.25.2"
  },
  "devDependencies": {
    "@rcompat/type": "^0.9.0"
  },
  "peerDependencies": {
    "primate": "workspace:^"
  },
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    },
    "#Component": {
      "apekit": "./src/private/client/Component.ts",
      "browser": "./lib/private/client/Component.js",
      "default": "./lib/private/server/Component.js"
    }
  },
  "exports": {
    ".": {
      "browser": "./lib/public/browser.js",
      "runtime": "./lib/public/runtime.js",
      "default": "./lib/public/default.js"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
