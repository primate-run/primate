{
  "name": "@primate/core",
  "version": "0.1.10",
  "description": "The universal web framework",
  "homepage": "https://primate.run",
  "bugs": "https://github.com/primate-run/primate/issues",
  "license": "MIT",
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/lib/private/defaults/*.html",
    "!/**/*.spec.*"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/core"
  },
  "scripts": {
    "build": "npm run clean && tsc && cp -a src/private/defaults lib/private",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build"
  },
  "dependencies": {
    "@rcompat/args": "^0.7.0",
    "@rcompat/array": "^0.7.0",
    "@rcompat/assert": "^0.1.0",
    "@rcompat/async": "^0.8.0",
    "@rcompat/build": "^0.12.2",
    "@rcompat/cli": "^0.9.0",
    "@rcompat/crypto": "^0.9.0",
    "@rcompat/fs": "^0.13.2",
    "@rcompat/function": "^0.8.0",
    "@rcompat/http": "^0.12.6",
    "@rcompat/kv": "^0.2.0",
    "@rcompat/package": "^0.12.0",
    "@rcompat/record": "^0.8.0",
    "@rcompat/runtime": "^0.5.0",
    "@rcompat/stdio": "^0.10.0",
    "@rcompat/sync": "^0.6.0",
    "pema": "workspace:^"
  },
  "type": "module",
  "imports": {
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    "./serve": {
      "apekit": "./src/public/serve/index.ts",
      "default": "./lib/public/serve/index.js"
    },
    "./error": "./lib/error/index.js",
    "./dispatch": "./lib/dispatch/index.js",
    "./frontend": {
      "apekit": "./src/public/Frontend.ts"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
