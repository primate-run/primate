{
  "name": "@primate/core",
  "version": "0.4.5",
  "description": "The universal web framework",
  "homepage": "https://primate.run",
  "bugs": "https://github.com/primate-run/primate/issues",
  "type": "module",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/primate-run/primate",
    "directory": "packages/core"
  },
  "files": [
    "/lib/**/*.js",
    "/lib/**/*.d.ts",
    "/lib/private/defaults/*.html",
    "!/**/*.spec.*"
  ],
  "scripts": {
    "build": "npm run clean && tsc && cp -a src/private/defaults lib/private",
    "clean": "rm -rf ./lib",
    "lint": "eslint .",
    "prepublishOnly": "npm run build",
    "test": "npm run build && npx proby"
  },
  "dependencies": {
    "@rcompat/args": "^0.11.0",
    "@rcompat/assert": "^0.6.0",
    "@rcompat/cli": "^0.14.0",
    "@rcompat/crypto": "^0.14.0",
    "@rcompat/dict": "^0.3.0",
    "@rcompat/fn": "^0.3.0",
    "@rcompat/fs": "^0.25.1",
    "@rcompat/http": "^0.21.0",
    "@rcompat/io": "^0.3.0",
    "@rcompat/is": "^0.4.0",
    "@rcompat/kv": "^0.6.0",
    "@rcompat/runtime": "^0.9.0",
    "@rcompat/string": "^0.13.0",
    "@rcompat/type": "^0.9.0",
    "esbuild": "^0.27.2",
    "pema": "workspace:^"
  },
  "imports": {
    "#i18n/index": {
      "apekit": "./src/private/i18n/index/client.ts",
      "browser": "./lib/private/i18n/index/client.js",
      "default": "./lib/private/i18n/index/server.js"
    },
    "#*": {
      "apekit": "./src/private/*.ts",
      "default": "./lib/private/*.js"
    }
  },
  "exports": {
    "./error": "./lib/error/index.js",
    "./dispatch": "./lib/dispatch/index.js",
    "./frontend": {
      "apekit": "./src/public/Frontend.ts"
    },
    "./*": {
      "apekit": "./src/public/*.ts",
      "default": "./lib/public/*.js"
    }
  }
}
