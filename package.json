{
  "name": "@primate/monorepo",
  "type": "module",
  "license": "MIT",
  "private": true,
  "scripts": {
    "build": "pnpm -r build",
    "build:packages": "pnpm -r --filter='./packages/**' build",
    "build:apps": "pnpm -r --filter='./apps/**' build",
    "eject": "node scripts/eject.js",
    "clean": "pnpm -r clean",
    "publish": "pnpm -r --access=public --otp=$(bw get totp npmjs.com) publish",
    "upgrade": "pnpm -r update -i -L",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "test": "pnpm -r test",
    "test:apps": "pnpm -r --filter='./apps/**' test",
    "sweep": "rm -rf node_modules pnpm-lock.yaml {app,package}s/*/node_modules apps/*/build && npm run clean",
    "knip": "knip"
  },
  "devDependencies": {
    "@rcompat/fs": "^0.25.2",
    "@rcompat/io": "^0.3.0",
    "@rcompat/test": "^0.8.0",
    "@types/deno": "^2.5.0",
    "@types/node": "^25.0.3",
    "apekit": "^0.0.42",
    "esbuild": "^0.27.2",
    "eslint": "^9.39.2",
    "knip": "^5.79.0",
    "proby": "^0.7.0",
    "typescript": "^5.9.3"
  }
}
